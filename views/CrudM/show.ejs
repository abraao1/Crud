                            <th scope="col">UF/CIDADE</th>
                            <th scope="col">FUN√á√ïES</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% data.forEach(function(details) { %>
                        <tr>
                        <td><%= details.name %></td>
                        <td><%= details.surname %></td>
                        <td><%= details.sexo %></td>
                        <td><%= details.cpf %></td>
                        <td><%= details.email %></td>
                        <td><%= details.numero %></td>
                        <td><%= details.cep %></td>
                        <td><%= details.ufCidade %></td>
                        <td><a href="/edit/matheus/<%= details._id %>">Editar</a> - <a href="/delete/matheus/<%= details._id %>">Deletar</a></td>
                        </tr>
                        <% }) %>
                    </tbody>
                </table>
                    <button type="submit" class="btn btn-dark btn-sm"><a href="/cadaster/matheus">VOLTAR</a></button>
            </div>
